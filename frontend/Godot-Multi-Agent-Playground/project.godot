config/icon="res://icon.svg"
<<<<<<<HEADrun/main_scene="res://scenes/test/test_scene_independent_objects.tscn"

[autoload]

DraftHouseLayout="*res://autoload/first_house_layout.gd"
DialogueManager="*res://addons/dialogue_manager/dialogue_manager.gd"
